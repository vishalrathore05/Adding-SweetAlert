<h1>Posts</h1>

<div id="posts">
  <% @posts.each do |post| %>
    <%= render post %>
    <p>
      <%= link_to "Show this post", post %>
      <%= link_to "Edit this post", edit_post_path(post) %>
      <%= link_to "Delete this post", post_path(post), method: :delete, data: { confirm_swal: "Are you sure you want to delete this post?" }, remote: true %>
    </p>
  <% end %>
</div>

<%= link_to "New post", new_post_path %>
 